{{! Template for the frost-range-picker component }}
<div class='column'>
  <div class='item starting-date'>
    <div class='title'>
      {{startingDateTitle}}
    </div>
    {{frost-date-time-picker
      defaultDate=startingDate
      defaultTime=startingTime
      dateValidator=startValidator
      timeValidator=startValidator
      onSelect=(action 'onSelect')
      onError=(action 'onError')
    }}
  </div>
  <div class='item starting-date'>
    <div class='title'>
      {{endingDateTitle}}
    </div>
    {{frost-date-time-picker
      defaultDate=endingDate
      defaultTime=endingTime
      dateValidator=endValidator
      timeValidator=endValidator
      onSelect=(action 'onSelect')
      onError=(action 'onError')
    }}
  </div>
</div>
